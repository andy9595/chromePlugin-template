{
  "manifest_version": 2,
  "name": "helloWorld",
  "version": "1.0",
  "description": "hello world 插件",
  "icons": {
    "128": "img/logo.png",
    "48": "img/logo.png",
    "16": "img/logo.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "tts"
  ],
  "content_scripts": [{
    "matches": ["https://www.baidu.com/", "http://www.baidu.com/"],
    "js": ["./test.js"],
    "css": ["./css/custom-style.css"],
    "run_at": "document_idle"
  }],
  "chrome_url_overrides": {
    "newtab": "new.html"
  },
  "options_page": "options.html"
}